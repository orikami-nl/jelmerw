<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(audi) -%>">
  <span class='title'>
    <%= audi.naam %>
    <span class="preview">&nbsp;</span>
  </span>
  <span class='actions'>
    <%= link_to refinery_icon_tag("application_go.png"), audi_path(audi),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    <%= link_to refinery_icon_tag("application_edit.png"), edit_admin_audi_path(audi),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), admin_audi_path(audi),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'shared.admin.delete', :title => audi.naam),
        :method => :delete %>
  </span>
</li>
